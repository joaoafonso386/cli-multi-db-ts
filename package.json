{
  "name": "node-life-api",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "clear": "rimraf ./dist/",
    "build": "npm run clear && tsc -p ./tsconfig.json && cp package.json package-lock.json dist/ && cp src/cli/db/heros.json dist/cli/db",
    "test:cli": "npx ts-mocha -p ./tsconfig.json src/cli/test/*",
    "dev:cli": "npx nodemon src/cli/main.ts --config src/cli/nodemon.json",
    "cli": "npx ts-node src/cli/main.ts",
    "dev:db": "npx tsx --watch-path=src/multi-db/ src/multi-db/main.ts",
    "test:db": "npx ts-mocha -p ./tsconfig.json src/multi-db/tests/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.5",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2"
  }
